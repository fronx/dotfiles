var
  antinode = require('antinode/antinode'),
  staticFolder = '/Users/alx/development/workspace/tweetcloud/static',
  settings = process.mixin(antinode.default_settings, {
    'log_level': 'ERROR',
    'default_host': {
      root: staticFolder
    },
    port: 4567,
    hosts: {
      '10.23.4.181  ': {
        root: staticFolder
      }
    }
  });

antinode.start(settings);
